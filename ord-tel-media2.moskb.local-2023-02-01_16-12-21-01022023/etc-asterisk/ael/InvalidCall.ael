// !!! OLD routing scheme: place for invalid call (drop it)
macro invalid-call(number) {
 NoOp(${leg1}>>${leg2} FUNCTION invalid-call(${number}));
 catch h { // whitout a catch, dialplan stops execution on hangup !!!
  hang=1;
  return;
 }

// NoOp(Invalid call number: ${number});
// Ringing();
// Wait(2);
 Progress();
 Set(CHANNEL(language)=${DEFAULT_LANG});
 &Event(invalid,${CONTEXT});
 Playback(invalid,noanswer);
 return;
} // macro invalid-call(number)
